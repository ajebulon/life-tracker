{
  "expo": {
    "name": "Life Tracker",
    "slug": "life-tracker",
    "version": "0.2",
    "orientation": "portrait",
    "icon": "./assets/life-tracker-icon.png",
    "splash": {
      "image": "./assets/life-tracker-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/life-tracker-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ajebulon.lifetracker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
